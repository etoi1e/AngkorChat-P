<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/joinFragment">

    <fragment
        android:id="@+id/joinFragment"
        android:name="com.example.angkorchatproto.pay.fragment.JoinFragment"
        android:label="fragment_join"
        tools:layout="@layout/fragment_join" >
        <action
            android:id="@+id/action_joinFragment_to_smsVerificationFragment"
            app:destination="@id/smsVerificationFragment" />
    </fragment>
    <fragment
        android:id="@+id/cardRegistrationFragment"
        android:name="com.example.angkorchatproto.pay.fragment.CardRegistrationFragment"
        android:label="fragment_card_registration"
        tools:layout="@layout/fragment_card_registration" />
    <fragment
        android:id="@+id/cardScanFragment"
        android:name="com.example.angkorchatproto.pay.fragment.CardScanFragment"
        android:label="fragment_card_scan"
        tools:layout="@layout/fragment_card_scan" />
    <fragment
        android:id="@+id/cardUseFragment"
        android:name="com.example.angkorchatproto.pay.fragment.CardUseFragment"
        android:label="fragment_card_use"
        tools:layout="@layout/fragment_card_use" />
    <fragment
        android:id="@+id/confirmSmsFragment"
        android:name="com.example.angkorchatproto.pay.fragment.ConfirmSmsFragment"
        android:label="fragment_confirm_sms"
        tools:layout="@layout/fragment_confirm_sms" />
    <fragment
        android:id="@+id/idCardFragment"
        android:name="com.example.angkorchatproto.pay.fragment.IdCardFragment"
        android:label="fragment_id_card"
        tools:layout="@layout/fragment_id_card" >
        <action
            android:id="@+id/action_idCardFragment_to_paymentRegistrationFragment"
            app:destination="@id/paymentRegistrationFragment" />
    </fragment>
    <fragment
        android:id="@+id/idVerificationFragment"
        android:name="com.example.angkorchatproto.pay.fragment.IdVerificationFragment"
        android:label="fragment_id_verification"
        tools:layout="@layout/fragment_id_verification" >
        <action
            android:id="@+id/action_idVerificationFragment_to_idCardFragment"
            app:destination="@id/idCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentPasswordFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PaymentPasswordFragment"
        android:label="fragment_payment_password"
        tools:layout="@layout/fragment_payment_password" >
        <action
            android:id="@+id/action_paymentPasswordFragment_to_payMainFragment"
            app:destination="@id/payMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentRegistrationFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PaymentRegistrationFragment"
        android:label="fragment_payment_registration"
        tools:layout="@layout/fragment_payment_registration" >
        <action
            android:id="@+id/action_paymentRegistrationFragment_to_cardScanFragment"
            app:destination="@id/cardScanFragment" />
    </fragment>
    <fragment
        android:id="@+id/smsVerificationFragment"
        android:name="com.example.angkorchatproto.pay.fragment.SmsVerificationFragment"
        android:label="fragment_sms_verification"
        tools:layout="@layout/fragment_sms_verification" >
        <action
            android:id="@+id/action_smsVerificationFragment_to_paymentPasswordFragment"
            app:destination="@id/paymentPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/payMainFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayMainFragment"
        android:label="fragment_pay_main"
        tools:layout="@layout/fragment_pay_main" >
        <action
            android:id="@+id/action_payMainFragment_to_payMyQrFragment"
            app:destination="@id/payMyQrFragment" />
        <action
            android:id="@+id/action_payMainFragment_to_payPointFragment"
            app:destination="@id/payPointFragment" />
        <action
            android:id="@+id/action_payMainFragment_to_payAngkorGiftCardFragment"
            app:destination="@id/payAngkorGiftCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/payMyQrFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayMyQrFragment"
        android:label="fragment_pay_my_qr"
        tools:layout="@layout/fragment_pay_my_qr" />
    <fragment
        android:id="@+id/payPointFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayPointFragment"
        android:label="PayPointFragment"
        tools:layout="@layout/fragment_pay_angkor_point">
        <action
            android:id="@+id/action_payPointFragment_to_payPointTopUpFragment"
            app:destination="@id/payPointTopUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/payPointTopUpFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayPointTopUpFragment"
        android:label="fragment_pay_point_top_up"
        tools:layout="@layout/fragment_pay_point_top_up" >
        <action
            android:id="@+id/action_payPointTopUpFragment_to_selectMethodFragment"
            app:destination="@id/selectMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectMethodFragment"
        android:name="com.example.angkorchatproto.pay.fragment.SelectMethodFragment"
        android:label="fragment_select_method"
        tools:layout="@layout/fragment_select_method" >
        <action
            android:id="@+id/action_selectMethodFragment_to_topUpCompleteFragment"
            app:destination="@id/topUpCompleteFragment" />
        <action
            android:id="@+id/action_selectMethodFragment_to_topUpSelectBankFragment"
            app:destination="@id/topUpSelectBankFragment" />
    </fragment>
    <fragment
        android:id="@+id/topUpCompleteFragment"
        android:name="com.example.angkorchatproto.pay.fragment.TopUpCompleteByCreditFragment"
        android:label="fragment_top_up_complete"
        tools:layout="@layout/fragment_top_up_complete_by_credit" >
        <action
            android:id="@+id/action_topUpCompleteFragment_to_payMainFragment"
            app:destination="@id/payMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/topUpSelectBankFragment"
        android:name="com.example.angkorchatproto.pay.fragment.TopUpSelectBankFragment"
        android:label="fragment_top_up_select_bank"
        tools:layout="@layout/fragment_top_up_select_bank" >
        <action
            android:id="@+id/action_topUpSelectBankFragment_to_checkBankFragment"
            app:destination="@id/checkBankFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkBankFragment"
        android:name="com.example.angkorchatproto.pay.fragment.CheckBankFragment"
        android:label="fragment_check_bank"
        tools:layout="@layout/fragment_check_bank" >
        <action
            android:id="@+id/action_checkBankFragment_to_topUpCompleteByAccountFragment"
            app:destination="@id/topUpCompleteByAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/topUpCompleteByAccountFragment"
        android:name="com.example.angkorchatproto.pay.fragment.TopUpCompleteByAccountFragment"
        android:label="fragment_top_up_complete_by_account"
        tools:layout="@layout/fragment_top_up_complete_by_account" >
        <action
            android:id="@+id/topUpCompleteByAccountFragment_to_payMainFragment"
            app:destination="@id/payMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/payAngkorGiftCardFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayAngkorGiftCardFragment"
        android:label="PayAngkorGiftCardFragment"
        tools:layout="@layout/fragment_pay_giftcard">
        <action
            android:id="@+id/action_payAngkorGiftCardFragment_to_payAngkorGiftCardCodeFragment"
            app:destination="@id/payAngkorGiftCardCodeFragment" />
        <action
            android:id="@+id/action_payAngkorGiftCardFragment_to_payAngkorGiftCardHistoryFragment"
            app:destination="@id/payAngkorGiftCardHistoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/payAngkorGiftCardCodeFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayAngkorGiftCardCodeFragment"
        android:label="PayAngkorGiftCardCodeFragment"
        tools:layout="@layout/fragment_pay_giftcard_code"/>
    <fragment
        android:id="@+id/payAngkorGiftCardHistoryFragment"
        android:name="com.example.angkorchatproto.pay.fragment.PayAngkorGiftCardHistoryFragment"
        android:label="PayAngkorGiftCardHistoryFragment"
        tools:layout="@layout/fragment_pay_giftcard_history"/>


</navigation>